{
    "name": "terminal42/dcawizard",
    "description": "dcaWizard extension for Contao Open Source CMS",
    "keywords": ["dca", "wizard", "dcawizard", "contao"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-dcawizard/issues",
        "source": "https://github.com/terminal42/contao-dcawizard"
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "^4.13 || ^5.0",
        "codefog/contao-haste": "^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\DcaWizardBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\DcaWizardBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "tools/ecs/vendor/bin/ecs check src/ --fix --ansi"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
