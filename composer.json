{
    "name": "terminal42/dcawizard",
    "description": "dcaWizard extension for Contao Open Source CMS",
    "keywords": ["dca", "wizard", "dcawizard", "contao"],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "http://terminal42.ch"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "^4.13 || ^5.0",
        "codefog/contao-haste": "5.x-dev@dev"
    },
    "autoload": {
        "classmap": ["classes", "widgets"]
    },
    "replace": {
        "contao-legacy/dcawizard": "self.version"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/dcawizard"
            }
        }
    }
}
